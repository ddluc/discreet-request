    "request": "^2.88.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.9",
    "@types/uuid": "^9.0.6",
    "jest": "^29.7.0",
    "redis": "^4.6.10",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
